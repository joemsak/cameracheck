<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(venue) -%>">
  <span class='title'>
    <%= venue.name %>
    <span class="preview">&nbsp;</span>
  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_go.png"), venue_url(venue),
        :title => t('.view_live'),
        :target => "_blank" %>
    <%= link_to refinery_icon_tag("application_edit.png"), edit_admin_venue_path(venue),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), admin_venue_path(venue),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :'data-confirm' => t('shared.admin.delete.message',
        :title => venue.name
      ),
        :'data-method' => :delete %>
  </span>
</li>
